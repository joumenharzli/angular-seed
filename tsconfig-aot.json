{
    "compilerOptions": {
        "rootDir": ".",
        "outDir": ".",
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es2015",
            "dom"
        ],
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "files": [
        "dist/aot/src/app/app.module.ts",
        "dist/aot/src/app/main.ts"
    ],
    "angularCompilerOptions": {
        "genDir": ".",
        "skipMetadataEmit": true
    }
}