{
  "name": "angular-plugnplay-seed",
  "version": "1.0.0",
  "description": "Maven-like seed for angular projects",
  "author": "Harzli Joumen",
  "license": "MIT",
  "scripts": {
    "clean": "gulp clean:dist",
    "lint": "gulp lint",
    "compile": "gulp build:global:compile:app",
    "build:dev": "gulp clean:dist && gulp build:global:copyresources && gulp build:dev:copylibs && gulp build:dev:index && gulp build:dev:systemjs && gulp build:global:compile:app",
    "build:prod": "gulp clean:dist && gulp build:global:copyresources && gulp build:prod:index && gulp build:prod:vendors && gulp build:global:compile:app && gulp build:prod:mainjs && gulp clean:app:js",
    "test:generatespec": "gulp test:generatespec",
    "test:compile": "gulp test:compile",
    "test": "karma start",
    "serve": "gulp serve && build:dev"
  },
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "reselect": "^2.5.4",
    "rxjs": "^5.0.2",
    "systemjs": "^0.19.41",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.53",
    "codelyzer": "^2.0.0-beta.4",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-live-server": "^0.0.30",
    "gulp-msg": "^0.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-sym": "^1.0.2",
    "gulp-tslint": "^7.0.1",
    "gulp-typescript": "^3.1.3",
    "gulp-uglify": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-istanbul": "^0.4.0",
    "karma-requirejs": "^1.1.0",
    "recursive-readdir": "^2.1.0",
    "requirejs": "^2.3.2",
    "run-sequence": "^1.2.2",
    "systemjs-builder": "^0.15.34",
    "traceur": "^0.0.111",
    "tslint": "^4.2.0",
    "typescript": "2.0.10"
  }
}
